{
  "extends": "next/core-web-vitals",
  "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "quotes": ["error", "single", { "avoidEscape": true }],
        "indent": ["error", 4],
        "max-len": ["error", { "code": 120 }],
        "@typescript-eslint/no-unused-vars": "warn",
        "@typescript-eslint/no-explicit-any": "warn"
    }
}
